
<header>
  <h1>Skribbly!</h1>

  <form id="leave-form" action="index.html">   
    <button >Leave Room</button>
  </form>
  
</header>

<div id="game-contents">

  <div id="player-container">
  </div>

  <div id="canvas-container">
    <canvas id="canvas" style="border: 10px solid white">  </canvas>
  </div>
  
  
  
  <div id="chat-container" style="position: relative;" >
  
    <main id="chat-main">
      <div id="chat-messages">
          <div class="message">
          </div>
      </div>
    </main>
  
  
    <div id="chat-form-container" style="position: absolute; bottom:0; width:100%">
      <input
        id="msg"
        type="text"
        placeholder="Message"
        required  
        autocomplete="off"
        style=" width:90%"
      />
    </div>
  </div>
</div>

<script src="/socket.io/socket.io.js"></script> 
<script src="/gameClient.js"></script>